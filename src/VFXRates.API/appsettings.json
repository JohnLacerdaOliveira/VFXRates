{
  "ConnectionStrings": {
    "FxRatesDb": "YourDatabaseConnectionString"
  },

  "AlphaVantage": {
    "ApiKey": "YourAlphaVantageApiKey"
  },

  "RabbitMQ": {
    "HostName": "localhost",
    "Port": "5672",
    "UserName": "YourRabbitMQUserName",
    "Password": "YourRabbitMQPassword",
    "Exchange": "YourExchangeName",
    "AllowedHosts": "*"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.EntityFrameworkCore": "Debug",
      "Microsoft.AspNetCore": "Debug"
    }
  },

  "Jwt": {
    "Secret": "your-very-strong-secret-key-should-be-long-enough",
    "Issuer": "VFXRatesApp",
    "Audience": "YourAppAudience",
    "ExpiresInMinutes": 600
  }
}